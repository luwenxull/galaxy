{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "only-arrow-functions": false,
        "quotemark": [true, "single"],
        "no-empty": false,
        "variable-name": [false],
        "no-unused-expression": false,
        "arrow-parens": false,
        "prefer-for-of": false,
        "max-line-length": [true, 120],
        "no-unused-variable": true
    },
    "rulesDirectory": []
}